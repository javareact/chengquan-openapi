{
  "name": "javareact/cq-api",
  "description": "橙券PHP-SDK,ChengQuan OpenApi SDK",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "橙券",
    "ChengQuan",
    "OpenApi",
    "SDK"
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "*",
    "psr/log": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-bcmath": "*"
  },
  "autoload": {
    "psr-4": {
      "JavaReact\\CQApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\CQApi\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "<9.0.0",
    "mockery/mockery": "^1.3",
    "friendsofphp/php-cs-fixer": "^2.16",
    "monolog/monolog": "^2.0"
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "phpunit --colors=always"
  }
}
